    },
    "description": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "venue": {
      "type": "string"
    },
    "time": {
      "type": "string"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
